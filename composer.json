{
    "name": "icekson/remote-services",    
    "description": "Remote services for build server API",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Itsekson",
            "email": "icekson85@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
		"zendframework/zend-cache": "~2.5",
        "psr/log": "1.0.*",
        "monolog/monolog": "~1.12",
        "doctrine/annotations": "~1.4",
        "zfr/rbac": "~1.2",
        "psr/container": "^1.0"
    },
     "require-dev": {
        "phpunit/phpunit" : "~6.0",
        "phpunit/phpunit-mock-objects": "^4.0"
    },
    "autoload": {
        "psr-4": {"Api\\": "src/Api", "ApiTest\\" : "tests/Api"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
