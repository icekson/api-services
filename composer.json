{
    "name": "icekson/remote-services",    
    "description": "Remote services for build server API",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Itsekson",
            "email": "icekson85@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "zendframework/zend-servicemanager": "2.3.*",
        "psr/log": "1.0.*",
        "monolog/monolog": "~1.12",
        "doctrine/annotations": "~1.2"
    },
     "require-dev": {
         "phpunit/phpunit" : ">=3.7.8",
        "phpunit/phpunit-mock-objects" : "1.2.1"                
    },
    "autoload": {
        "psr-4": {"Api\\": "src/Api", "ApiTest\\" : "tests/Api"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
